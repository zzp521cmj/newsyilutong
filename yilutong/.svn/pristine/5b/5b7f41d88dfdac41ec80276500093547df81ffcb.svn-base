package com.jy.model;

public  class MonitorSet {
	private int smonitor_id;//监控id
	private int smonitor_line;//线路监控：1开。0关
	private int smonitor_distance;//距离监控：1开，0关
	private int smonitor_point;//停靠点监控：1开，0关
	private int smonitor_trantime;//发运时间报警：1开，0关
	private int smonitor_cartime;//车辆运行时间报警：1开，0关
	private int smonitor_pointtime;//节点作业时间报警：1开。0关
	public int getSmonitor_id() {
		return smonitor_id;
	}
	public void setSmonitor_id(int smonitor_id) {
		this.smonitor_id = smonitor_id;
	}
	public int getSmonitor_line() {
		return smonitor_line;
	}
	public void setSmonitor_line(int smonitor_line) {
		this.smonitor_line = smonitor_line;
	}
	public int getSmonitor_distance() {
		return smonitor_distance;
	}
	public void setSmonitor_distance(int smonitor_distance) {
		this.smonitor_distance = smonitor_distance;
	}
	public int getSmonitor_point() {
		return smonitor_point;
	}
	public void setSmonitor_point(int smonitor_point) {
		this.smonitor_point = smonitor_point;
	}
	public int getSmonitor_trantime() {
		return smonitor_trantime;
	}
	public void setSmonitor_trantime(int smonitor_trantime) {
		this.smonitor_trantime = smonitor_trantime;
	}
	public int getSmonitor_cartime() {
		return smonitor_cartime;
	}
	public void setSmonitor_cartime(int smonitor_cartime) {
		this.smonitor_cartime = smonitor_cartime;
	}
	public int getSmonitor_pointtime() {
		return smonitor_pointtime;
	}
	public void setSmonitor_pointtime(int smonitor_pointtime) {
		this.smonitor_pointtime = smonitor_pointtime;
	}
}

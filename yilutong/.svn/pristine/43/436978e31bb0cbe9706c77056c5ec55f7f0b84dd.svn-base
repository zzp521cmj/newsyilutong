package com.jy.model;

public class Function {
	private int function_id;
	private String function_name;
	private int function_menuid;
	private int function_order;
	
	public Function() {
		super();
		// TODO Auto-generated constructor stub
	}
	public int getFunction_id() {
		return function_id;
	}
	public void setFunction_id(int function_id) {
		this.function_id = function_id;
	}
	public String getFunction_name() {
		return function_name;
	}
	public void setFunction_name(String function_name) {
		this.function_name = function_name;
	}
	public int getFunction_menuid() {
		return function_menuid;
	}
	public void setFunction_menuid(int function_menuid) {
		this.function_menuid = function_menuid;
	}
	public int getFunction_order() {
		return function_order;
	}
	public void setFunction_order(int function_order) {
		this.function_order = function_order;
	}
	
}

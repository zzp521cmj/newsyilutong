package com.jy.model;

public class Linstandard_truck {
	private String jltid;//主
	private String lid;//线路id
	private String tid;//车辆id
	private String tname;//车辆名字
	public String getJltid() {
		return jltid;
	}
	public void setJltid(String jltid) {
		this.jltid = jltid;
	}
	public String getLid() {
		return lid;
	}
	public void setLid(String lid) {
		this.lid = lid;
	}
	public String getTid() {
		return tid;
	}
	public void setTid(String tid) {
		this.tid = tid;
	}
	public String getTname() {
		return tname;
	}
	public void setTname(String tname) {
		this.tname = tname;
	}
	
}
